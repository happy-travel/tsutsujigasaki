{   "AllowedHosts": "*",
  "ConnectionStrings": {
    "Tsutsujigasaki": "Server={0};Port={1};Database=tsutsujigasaki;Userid={2};Password={3};"
  },
  "CurrencyLayer": {
    "Options": "tsutsujigasaki/currency-layer"
  },
  "Database": {
    "Options": "tsutsujigasaki/connection-string"
  },
  "Jaeger": {
    "AgentHost": "JAEGER_AGENT_HOST",
    "AgentPort": "JAEGER_AGENT_PORT"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Trace",
      "Microsoft.Hosting.Lifetime": "Trace",
      "HappyTravel.StdOutLogger.HttpContextLoggingMiddleware": "Information"
    },
    "Sentry": {
      "Endpoint": "HTDC_TSUTSUJIGASAKI_SENTRY_ENDPOINT"
    }
  },
  "Redis": {
    "Endpoint": "HTDC_REDIS_HOST"
  },
  "Vault": {
    "Endpoint": "HTDC_VAULT_ENDPOINT",
    "Engine": "secrets",
    "Role": "tsutsujigasaki",
    "Token": "HTDC_VAULT_TOKEN"
  }
}
